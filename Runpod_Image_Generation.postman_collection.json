{
  "info": {
    "name": "Runpod Image Generation API",
    "description": "API collection for testing Runpod image generation with Eromify backend",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{AUTH_TOKEN}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "BASE_URL",
      "value": "http://localhost:3001",
      "type": "string"
    },
    {
      "key": "AUTH_TOKEN",
      "value": "YOUR_AUTH_TOKEN_HERE",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{BASE_URL}}/health",
          "host": ["{{BASE_URL}}"],
          "path": ["health"]
        },
        "description": "Check if the backend server is running"
      }
    },
    {
      "name": "Generate Image - Simple Portrait 1:1",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"prompt\": \"beautiful woman, professional photo, high quality, 8k\",\n  \"aspectRatio\": \"1:1\",\n  \"numInferenceSteps\": 28,\n  \"guidanceScale\": 7.5\n}"
        },
        "url": {
          "raw": "{{BASE_URL}}/api/content/generate-image-runpod",
          "host": ["{{BASE_URL}}"],
          "path": ["api", "content", "generate-image-runpod"]
        },
        "description": "Generate a simple 1:1 portrait image"
